<form id="topicForm" method="post" action="/web/especialidad/<%= especialidad.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= especialidad.id %>">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <th class="cell-stat text-center">id</th>
        <th class="cell-stat text-center">especialidad</th>
        <th class="cell-stat text-center">creacion</th>
        <th class="cell-stat text-center">modifica</th>
        <th class="cell-stat text-center">borrado</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td class="text-center">
            <%= especialidad.id %>
          </td>
          <td class="text-center">
            <label for="especialidad" class="control-label">Especialidad</label>
            <input type="text" class="form-control" id="especialidad" name="especialidad" value="<%=especialidad.especialidad%>" required="true" title="Ingrese especialidad" placeholder="<especialidad>">
            <%= especialidad.especialidad %>
          </td>
          
          <td class="text-center"><%= especialidad.creacion %></td>
          <td class="text-center"><%= especialidad.modifica %></td>
          <td class="text-center"><%= especialidad.borrado %></td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar especialidad
            </button>
          </td>
          <td>
            <a href="/web/especialidad/<%= especialidad.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Delete</button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
