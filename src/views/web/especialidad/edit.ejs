<form id="topicForm" method="post" action="/web/especialidad/<%= especialidad.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= especialidad.id %>">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <td class="cell-stat text-center">Id</td>
        <td class="text-center">
          <%= especialidad.id %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Especialidad</td>
        <td class="text-center">
          <input type="text" class="form-control" id="especialidad" name="especialidad" value="<%=especialidad.especialidad%>" required="true" title="Ingrese especialidad" placeholder="<especialidad>">
          <%= especialidad.especialidad %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Creacion</td>
        <td class="text-center"><%= especialidad.fechaCrea %></td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Modifica</td>
        <td class="text-center"><%= especialidad.fechaModifica %></td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Borrado</td>
        <td class="text-center"><%= especialidad.fechaBorra %></td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Accion</td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Accion</td>
      </tr>

    </thead>
    <tbody>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar especialidad
            </button>
          </td>
          <td>
            <a href="/web/especialidad/<%= especialidad.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Delete</button>
              </p>
            </a>
          </td>
    </tbody>
  </table>
</form>
