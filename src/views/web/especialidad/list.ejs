<table class="table table-bordred table-striped">
  <thead>
    <tr>
      <th class="cell-stat text-center">id</th>
      <th class="cell-stat text-center">especialidad</th>
      <th class="cell-stat text-center">creacion</th>
      <th class="cell-stat text-center">Modifica</th>
      <th class="cell-stat text-center">Borrado</th>
      <th class="cell-stat text-center">Accion</th>
      <th class="cell-stat text-center">Accion</th>
    </tr>
  </thead>
  <tbody>
    <% var i; for (i=0; i < especialidades.length; i++) { %>
      <tr>
        <td class="text-center"><%= especialidades[i].id %></td>
        <td class="text-center"><%= especialidades[i].especialidad %></td>
        <td class="text-center"><%= especialidades[i].creacion %></td>
        <td class="text-center"><%= especialidades[i].modifica %></td>
        <td class="text-center"><%= especialidades[i].borrado %></td>
        <td>
          <a href="/web/especialidad/<%= especialidades[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Edit</button>
            </p>
          </a>
        </td>
        <td>
          <a href="/web/especialidad/<%= especialidades[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Delete</button>
            </p>
          </a>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>
