<table class="table table-bordred table-striped">
  <thead>
    <tr>
      <th class="cell-stat text-center">id</th>
      <th class="cell-stat text-center">foto</th>
      <th class="cell-stat text-center">codArchivoFotografico</th>
      <th class="cell-stat text-center">numRollo</th>
      <th class="cell-stat text-center">numFoto</th>
      <th class="cell-stat text-center">fotografo</th>
      <th class="cell-stat text-center">fecha</th>
      <th class="cell-stat text-center">creacion</th>
      <th class="cell-stat text-center">modifica</th>
      <th class="cell-stat text-center">borrado</th>
      <th class="cell-stat text-center">ObraId</th>
      <th class="cell-stat text-center">Accion</th>
      <th class="cell-stat text-center">Accion</th>
    </tr>
  </thead>
  <tbody>
    <% var i; for (i=0; i < fotografias.length; i++) { %>
      <tr>
        <td class="text-center"><%= fotografias[i].id %></td>
        <td class="text-center"><%= fotografias[i].foto %></td>
        <td class="text-center"><%= fotografias[i].codArchivoFotografico %></td>
        <td class="text-center"><%= fotografias[i].numRollo %></td>
        <td class="text-center"><%= fotografias[i].numFoto %></td>
        <td class="text-center"><%= fotografias[i].fotografo %></td>
        <td class="text-center"><%= fotografias[i].fecha %></td>
        <td class="text-center"><%= fotografias[i].creacion %></td>
        <td class="text-center"><%= fotografias[i].modifica %></td>
        <td class="text-center"><%= fotografias[i].borrado %></td>
        <td class="text-center"><%= fotografias[i].ObraId %></td>
        <td>
          <a href="/web/fotografia/<%= fotografias[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Edit</button>
            </p>
          </a>
        </td>
        <td>
          <a href="/web/fotografia/<%= fotografias[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Delete</button>
            </p>
          </a>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>
