<form id="topicForm" method="post" action="/web/tecnicasArte/<%= tecnicaArte.id %>" >
  <input type="hidden" name="_method" value="put">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <th class="cell-stat text-center">ID</th>
        <th class="cell-stat text-center">TECNICA ARTE</th>
        <th class="cell-stat text-center">FECHA CREA</th>
        <th class="cell-stat text-center">FECHA MODIFICA</th>
        <th class="cell-stat text-center">FECHA BORRA</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td class="text-center">
            <%= tecnicaArte.id %>
          </td>
          <td class="text-center">
            <label for="museo" class="control-label">Id</label>
            <input type="text" class="form-control" id="museo" name="museo.museo" value="<%=tecnicaArte.tecnicaArte%>" required="true" title="Ingrese Tecnica Arte" placeholder="<museo>">
            <%= tecnicaArte.tecnicaArte %>
          </td>
          
          <td class="text-center"><%= tecnicaArte.fechaCrea %></td>
          <td class="text-center"><%= tecnicaArte.fechaModifica %></td>
          <td class="text-center"><%= tecnicaArte.fechaBorra %></td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/tecnicasArte/<%= tecnicaArte.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancelar</button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
