<form id="topicForm" method="post" action="/web/museo/<%= museo.id %>" >
  <input type="hidden" name="_method" value="put">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <th class="cell-stat text-center">id</th>
        <th class="cell-stat text-center">museo</th>
        <th class="cell-stat text-center">direccion</th>
        <th class="cell-stat text-center">telefono</th>
        <th class="cell-stat text-center">fechaCrea</th>
        <th class="cell-stat text-center">fechaModifica</th>
        <th class="cell-stat text-center">fechaBorra</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td class="text-center">
            <%= museo.id %>
          </td>
          <td class="text-center">
            <label for="museo" class="control-label">Id</label>
            <input type="text" class="form-control" id="museo" name="museo.museo" value="<%=museo.museo%>" required="true" title="Ingrese museo" placeholder="<museo>">
            <%= museo.museo %>
          </td>
          <td class="text-center">
            <label for="direccion" class="control-label">Id</label>
            <input type="text" class="form-control" id="direccion" name="museo.direccion" value="<%=museo.direccion%>" required="true" title="Ingrese direccion" placeholder="<direccion>">
            <%= museo.direccion %>
          </td>
          <td class="text-center">
            <label for="direccion" class="control-label">Telefono</label>
            <input type="text" class="form-control" id="telefono" name="museo.telefono" value="<%=museo.telefono%>" required="true" title="Ingrese telefono" placeholder="<telefono>">
            <%= museo.telefono %>
          </td>
          <td class="text-center"><%= museo.fechaCrea %></td>
          <td class="text-center"><%= museo.fechaModifica %></td>
          <td class="text-center"><%= museo.fechaBorra %></td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/museo/<%= museo.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancel
                </button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
