<a href="/web/museo/add %>" class="btn btn-info" role="button">
  <p data-placement="top" data-toggle="tooltip" title="Edit">
    <button type="button" class="btn btn-info">Agregar</button>
  </p>
</a>
<table class="table table-bordred table-striped">
  <thead>
    <tr>
      <th class="cell-stat text-center">id</th>
      <th class="cell-stat text-center">museo</th>
      <th class="cell-stat text-center">direccion</th>
      <th class="cell-stat text-center">telefono</th>
      <th class="cell-stat text-center">fechaCrea</th>
      <th class="cell-stat text-center">fechaModifica</th>
      <th class="cell-stat text-center">fechaBorra</th>
      <th class="cell-stat text-center">Accion</th>
      <th class="cell-stat text-center">Accion</th>
    </tr>
  </thead>
  <tbody>
    <% var i; for (i=0; i < museos.length; i++) { %>
      <tr>
        <td class="text-center"><%= museos[i].id %></td>
        <td class="text-center"><%= museos[i].museo %></td>
        <td class="text-center"><%= museos[i].direccion %></td>
        <td class="text-center"><%= museos[i].telefono %></td>
        <td class="text-center"><%= museos[i].fechaCrea %></td>
        <td class="text-center"><%= museos[i].fechaModifica %></td>
        <td class="text-center"><%= museos[i].fechaBorra %></td>
        <td>
          <a href="/web/museo/<%= museos[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Edit</button>
            </p>
          </a>
        </td>
        <td>
          <form id="topicForm" method="post" action="/web/museo/<%= museos[i].id %>" >
            <input type="hidden" name="_method" value="delete">
            <input type="hidden" name="id" value="<%= museos[i].id %>">
            <p data-placement="top" data-toggle="tooltip" title="Delete">
              <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
                Delete
              </button>
            </p>
          </form>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>
