<form id="topicForm" method="post" action="/web/ubicacion/<%= ubicacion.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= ubicacion.id %>">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <th class="cell-stat text-center">id</th>
        <th class="cell-stat text-center">espacio</th>
        <th class="cell-stat text-center">inmueble</th>
        <th class="cell-stat text-center">propietario</th>
        <th class="cell-stat text-center">creacion</th>
        <th class="cell-stat text-center">modifica</th>
        <th class="cell-stat text-center">borrado</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td class="text-center">
            <%= ubicacion.id %>
          </td>
          <td class="text-center">
            <label for="espacio" class="control-label">Espacio</label>
            <input type="text" class="form-control" id="espacio" name="espacio" value="<%=ubicacion.espacio%>" required="true" title="Ingrese ubicacion" placeholder="<espacio>">
            <%= ubicacion.espacio %>
          </td>
          <td class="text-center">
            <label for="inmueble" class="control-label">Inmueble</label>
            <input type="text" class="form-control" id="inmueble" name="inmueble" value="<%=ubicacion.inmueble%>" required="true" title="Ingrese ubicacion" placeholder="<inmueble>">
            <%= ubicacion.inmueble %>
          </td>
          <td class="text-center">
            <label for="propietario" class="control-label">Propietario</label>
            <input type="text" class="form-control" id="propietario" name="propietario" value="<%=ubicacion.propietario%>" required="true" title="Ingrese ubicacion" placeholder="<propietario>">
            <%= ubicacion.propietario %>
          </td>
          <td class="text-center"><%= ubicacion.creacion %></td>
          <td class="text-center"><%= ubicacion.modifica %></td>
          <td class="text-center"><%= ubicacion.borrado %></td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/ubicacion/<%= ubicacion.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancel
                </button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
