<table class="table table-bordred table-striped">
  <thead>
    <tr>
      <th class="cell-stat text-center">id</th>
      <th class="cell-stat text-center">espacio</th>
      <th class="cell-stat text-center">inmueble</th>
      <th class="cell-stat text-center">propietario</th>
      <th class="cell-stat text-center">creacion</th>
      <th class="cell-stat text-center">modifica</th>
      <th class="cell-stat text-center">borrado</th>
      <th class="cell-stat text-center">ObraId</th>
      <th class="cell-stat text-center">LugarId</th>
      <th class="cell-stat text-center">Accion</th>
      <th class="cell-stat text-center">Accion</th>
    </tr>
  </thead>
  <tbody>
    <% var i; for (i=0; i < ubicaciones.length; i++) { %>
      <tr>
        <td class="text-center"><%= ubicaciones[i].id %></td>
        <td class="text-center"><%= ubicaciones[i].espacio %></td>
        <td class="text-center"><%= ubicaciones[i].inmueble %></td>
        <td class="text-center"><%= ubicaciones[i].propietario %></td>
        <td class="text-center"><%= ubicaciones[i].creacion %></td>
        <td class="text-center"><%= ubicaciones[i].modifica %></td>
        <td class="text-center"><%= ubicaciones[i].borrado %></td>
        <td class="text-center"><%= ubicaciones[i].ObraId %></td>
        <td class="text-center"><%= ubicaciones[i].LugarId %></td>
        <td>
          <a href="/web/ubicacion/<%= ubicaciones[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Edit</button>
            </p>
          </a>
        </td>
        <td>
          <a href="/web/ubicacion/<%= ubicaciones[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Delete</button>
            </p>
          </a>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>
