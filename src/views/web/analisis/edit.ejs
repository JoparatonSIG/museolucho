<form id="topicForm" method="post" action="/web/analisis/<%= analisis.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= analisis.id %>">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <td class="cell-stat text-center">id</td>
        <td class="text-center">
          <%= analisis.id %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Tipo de Analisis</td>
        <td class="text-center">
          <select name="tipoAnalisisSele">
            <% var j; for (j=0; j < select.length; j++) { %>
            <% if (analisis.TipoAnalisisId == select[j].id) {%>
              <option value="<%= select[j].id %>" selected ><%= select[j].tipo %> /
              <%= select[j].subTipo %> </option>
           <% } else {%>
             <option value="<%= select[j].id %>"><%= select[j].tipo %> /
               <%= select[j].subTipo %>
            </option>
           <% } %>
          <% } %>
          </select>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">analisis</td>
        <td class="text-center">
          <input type="text" class="form-control" id="analisis" name="analisis" value="<%=analisis.analisis%>" required="true" title="Ingrese analisis" placeholder="<analisis>">
          <%= analisis.analisis %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">ObraId</td>
        <td class="text-center">
          <%= analisis.ObraId %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">creacion</td>
        <td class="text-center"><%= analisis.fechaCrea %></td>
      </tr>
      <tr>
        <td class="cell-stat text-center">modifica</td>
        <td class="text-center"><%= analisis.fechaModifica %></td>

      </tr>
      <tr>
        <td class="cell-stat text-center">borrado</td>
          <td class="text-center"><%= analisis.fechaBorra %></td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Accion</td>
        <td>
          <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
            Modificar analisis
          </button>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Accion</td>
        <td>
          <a href="/web/analisis/<%= analisis.id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Delete</button>
            </p>
          </a>
        </td>
      </tr>
    </thead>
    
  </table>
</form>
