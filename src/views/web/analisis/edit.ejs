<form id="topicForm" method="post" action="/web/analisis/<%= analisis.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= analisis.id %>">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <th class="cell-stat text-center">id</th>
        <th class="cell-stat text-center">analisis</th>
        <th class="cell-stat text-center">ObraId</th>
        <th class="cell-stat text-center">creacion</th>
        <th class="cell-stat text-center">modifica</th>
        <th class="cell-stat text-center">borrado</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td class="text-center">
            <%= analisis.id %>
          </td>
          <td class="text-center">
            <label for="analisis" class="control-label">Analisis</label>
            <input type="text" class="form-control" id="analisis" name="analisis" value="<%=analisis.analisis%>" required="true" title="Ingrese analisis" placeholder="<analisis>">
            <%= analisis.analisis %>
          </td>
          <td class="text-center">
            <%= analisis.ObraId %>
          </td>
          <td class="text-center"><%= analisis.creacion %></td>
          <td class="text-center"><%= analisis.modifica %></td>
          <td class="text-center"><%= analisis.borrado %></td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar analisis
            </button>
          </td>
          <td>
            <a href="/web/analisis/<%= analisis.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Delete</button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
