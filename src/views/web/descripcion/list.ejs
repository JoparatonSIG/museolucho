<a href="/web/descripcion/add %>" class="btn btn-info" role="button">
  <p data-placement="top" data-toggle="tooltip" title="Edit">
    <button type="button" class="btn btn-info">Agregar</button>
  </p>
</a>
<table class="table table-bordred table-striped">
  <thead>
    <tr>
      <th class="cell-stat text-center">id</th>
      <th class="cell-stat text-center">marcasInscripciones</th>
      <th class="cell-stat text-center">alto</th>
      <th class="cell-stat text-center">ancho</th>
      <th class="cell-stat text-center">longitud</th>
      <th class="cell-stat text-center">profundidad</th>
      <th class="cell-stat text-center">diametro</th>
      <th class="cell-stat text-center">espesor</th>
      <th class="cell-stat text-center">peso</th>
      <th class="cell-stat text-center">observaciones</th>
      <th class="cell-stat text-center">descripcion</th>
      <th class="cell-stat text-center">ObraId</th>
      <th class="cell-stat text-center">creacion</th>
      <th class="cell-stat text-center">Modifica</th>
      <th class="cell-stat text-center">Borrado</th>
      <th class="cell-stat text-center">Accion</th>
      <th class="cell-stat text-center">Accion</th>
    </tr>
  </thead>
  <tbody>
    <% var i; for (i=0; i < descripciones.length; i++) { %>
      <tr>
        <td class="text-center"><%= descripciones[i].id %></td>
        <td class="text-center"><%= descripciones[i].marcasInscripciones %></td>
        <td class="text-center"><%= descripciones[i].alto %></td>
        <td class="text-center"><%= descripciones[i].ancho %></td>
        <td class="text-center"><%= descripciones[i].longitud %></td>
        <td class="text-center"><%= descripciones[i].profundidad %></td>
        <td class="text-center"><%= descripciones[i].diametro %></td>
        <td class="text-center"><%= descripciones[i].espesor %></td>
        <td class="text-center"><%= descripciones[i].peso %></td>
        <td class="text-center"><%= descripciones[i].observaciones %></td>
        <td class="text-center"><%= descripciones[i].descripcion %></td>
        <td class="text-center"><%= descripciones[i].ObraId %></td>
        <td class="text-center"><%= descripciones[i].FechaCrea %></td>
        <td class="text-center"><%= descripciones[i].FechaModifica %></td>
        <td class="text-center"><%= descripciones[i].FechaBorra %></td>
        <td>
          <a href="/web/descripcion/<%= descripciones[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Edit</button>
            </p>
          </a>
        </td>
        <td>
          <form id="topicForm" method="post" action="/web/descripcion/<%= descripciones[i].id %>" >
            <input type="hidden" name="_method" value="delete">
            <input type="hidden" name="id" value="<%= descripciones[i].id %>">
            <p data-placement="top" data-toggle="tooltip" title="Delete">
              <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
                Delete
              </button>
            </p>
          </form>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>


<form id="topicForm" method="get" action="/web/descripcion/%>" >
    <% if (count > limitPage) { %>
      <% if (totalPage <= 8) { %>
        <% var j; for (j=0; j <= totalPage; j++) { %>
          <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
            <%=j%>
          </button>
        <% } %>
      <% } else { %>
        <% var j; for (j=1; j <= 3; j++) { %>
          <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
            <%=j%>
          </button>
        <% } %>
        <p>...</p>
        <% for (j=totalPage-3; j <= totalPage+1; j++) { %>
          <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
            <%=j%>
          </button>
        <% } %>
      <% } %>
    <% } %>
</form>
