    <tbody>

          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar descripcion
            </button>
          </td>
          <td>
            <a href="/web/descripcion/<%= descripcion.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Delete</button>
              </p>
            </a>
          </td>
    </tbody>
  </table>
</form>
