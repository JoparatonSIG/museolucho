<form id="topicForm" method="post" action="/web/nivel/<%= nivel.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= nivel.id %>">
  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <th class="cell-stat text-center">ID</th>
        <th class="cell-stat text-center">CATEGORIA</th>
        <th class="cell-stat text-center">FECHA CREA</th>
        <th class="cell-stat text-center">FECHA MODIFICA</th>
        <th class="cell-stat text-center">FECHA BORRA</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td class="text-center">
            <%= nivel.id %>
          </td>
          <td class="text-center">
            <label for="categoria" class="control-label">Id</label>
            <input type="text" class="form-control" id="categoria" name="categoria" value="<%=nivel.categoria%>" required="true" title="Ingrese el Nivel" placeholder="<categoria>">
            <%= nivel.categoria %>
          </td>
          
          <td class="text-center"><%= nivel.fechaCrea %></td>
          <td class="text-center"><%= nivel.fechaModifica %></td>
          <td class="text-center"><%= nivel.fechaBorra %></td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/nivel/<%= nivel.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancelar</button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
