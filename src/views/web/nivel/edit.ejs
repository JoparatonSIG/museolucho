<form id="topicForm" method="post" action="/web/nivel/<%= nivel.id %>" class="form-horizontal" role="form">
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= nivel.id %>">
  <div class="form-group">
    <label class="control-label col-sm-2" for="usuario"/> Id de Nivel <%= nivel.id %>
  </div>
  <div class="form-group">  
    <label for="categoria" class="control-label col-sm-2">Id</label>
    <div class="col-sm-10" >
      <input type="text" class="form-control" id="categoria" name="categoria" value="<%=nivel.categoria%>" required="true" title="Ingrese el Nivel" placeholder="<categoria>">
      <%= nivel.categoria %>
    </div>
  </div>          
  <div class="form-group">  
    <label class="control-label col-sm-2" for="email">Fecha de Creacion:</label>
    <div class="col-sm-10" >
      <%= nivel.fechaCrea %>
    </div>
  </div> 
  <div class="form-group">
    <label class="control-label col-sm-2" for="email">Fecha de Modificacion:</label>  
     <%= nivel.fechaModifica %>
  </div>
  <div class="form-group">
    <label class="control-label col-sm-2" for="email">Fecha de Eliminacion:</label>  
      <%= nivel.fechaBorra %>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-2" >
      <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
        Modificar
      </button>
    </div>
  </div>
  <div class="form-group">
    <div class="col-sm-offset-2 col-sm-2" >
      <a href="/web/nivel/<%= nivel.id %>" class="btn btn-info" role="button">
        <p data-placement="top" data-toggle="tooltip" title="Edit">
          <button type="button" class="btn btn-info">Cancelar</button>
        </p>
      </a>
    </div>
  </div>        
</form>
