<form id="topicForm" method="post" action="/web/accesorio/<%= accesorio.id %>" >
    <table class="table table-bordred table-striped">
      <tbody>
        <tr>
          <td class="cell-stat text-center">objetoCodigo</td>
            <td class="text-center">
              <input type="text" class="form-control" id="objetoCodigo" name="objetoCodigo" value="<%=accesorio.objetoCodigo%>" required="true" title="Ingrese objetoCodigo" placeholder="<objetoCodigo>">
              <%= accesorio.objetoCodigo %>
            </td>
        </tr>
          <tr>
          <td class="cell-stat text-center">Relacion</td>
            <td class="text-center">
              <input type="text" class="form-control" id="Relacion" name="Relacion" value="<%=accesorio.Relacion%>" required="true" title="Ingrese Relacion" placeholder="<Relacion>">
              <%= accesorio.Relacion %>
            </td>
          </tr>
      </tbody>
    </table>
    <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
      Agregar
    </button>
</form>
