<a href="/web/accesorio/add %>" class="btn btn-info" role="button">
  <p data-placement="top" data-toggle="tooltip" title="Edit">
    <button type="button" class="btn btn-info">Agregar</button>
  </p>
</a>
<table class="table table-bordred table-striped">
  <thead>
    <tr>
      <th class="cell-stat text-center">id</th>
      <th class="cell-stat text-center">objetoCodigo</th>
      <th class="cell-stat text-center">relacion</th>
      <th class="cell-stat text-center">ObraId</th>
      <th class="cell-stat text-center">creacion</th>
      <th class="cell-stat text-center">Modifica</th>
      <th class="cell-stat text-center">Borrado</th>
      <th class="cell-stat text-center">Accion</th>
      <th class="cell-stat text-center">Accion</th>
    </tr>
  </thead>
  <tbody>
    <% var i; for (i=0; i < accesorios.length; i++) { %>
      <tr>
        <td class="text-center"><%= accesorios[i].id %></td>
        <td class="text-center"><%= accesorios[i].objetoCodigo %></td>
        <td class="text-center"><%= accesorios[i].relacion %></td>
        <td class="text-center"><%= accesorios[i].ObraId %></td>
        <td class="text-center"><%= accesorios[i].creacion %></td>
        <td class="text-center"><%= accesorios[i].modifica %></td>
        <td class="text-center"><%= accesorios[i].borrado %></td>
        <td>
          <a href="/web/accesorio/<%= accesorios[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Edit</button>
            </p>
          </a>
        </td>
        <td>
          <form id="topicForm" method="post" action="/web/accesorio/<%= accesorios[i].id %>" >
            <input type="hidden" name="_method" value="delete">
            <input type="hidden" name="id" value="<%= accesorios[i].id %>">
            <p data-placement="top" data-toggle="tooltip" title="Delete">
              <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
                Delete
              </button>
            </p>
          </form>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>

<form id="topicForm" method="get" action="/web/accesorio/%>" >
    <% if (count > limitPage) { %>
      <% if (totalPage <= 8) { %>
        <% var j; for (j=0; j <= totalPage; j++) { %>
          <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
            <%=j%>
          </button>
        <% } %>
      <% } else { %>
        <% var j; for (j=1; j <= 3; j++) { %>
          <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
            <%=j%>
          </button>
        <% } %>
        <p>...</p>
        <% for (j=totalPage-3; j <= totalPage+1; j++) { %>
          <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
            <%=j%>
          </button>
        <% } %>
      <% } %>
    <% } %>
</form>
