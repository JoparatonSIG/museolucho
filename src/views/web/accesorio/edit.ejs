<form id="topicForm" method="post" action="/web/accesorio/<%= accesorio.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= accesorio.id %>">

  <table class="table table-bordred table-striped">
    <tbody>
      <tr>
        <td class="cell-stat text-center">id</td>
        <td class="text-center">
            <%= accesorio.id %>
        </td>
      </tr>
      <tr>
          <td class="cell-stat text-center">Objeto Codigo</td>
          <td class="text-center">
            <input type="text" class="form-control" id="objetoCodigo" name="objetoCodigo" value="<%=accesorio.objetoCodigo%>" required="true" title="Ingrese objetoCodigo" placeholder="<objetoCodigo>">
            <%= accesorio.objetoCodigo %>
          </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Relacion</td>
        <td class="text-center">
          <input type="text" class="form-control" id="relacion" name="relacion" value="<%=accesorio.relacion%>" required="true" title="Ingrese relacion" placeholder="<relacion>">
          <%= accesorio.relacion %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">ObraId</td>
        <td class="text-center">
          <%= accesorio.ObraId %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">FechaCrea</td>
        <td class="text-center">
          <%= accesorio.fechaCrea %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">FechaModifica</td>
        <td class="text-center">
          <%= accesorio.fechaModifica %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">FechaBorra</td>
        <td class="text-center">
          <%= accesorio.fechaBorra %>
        </td>
      <tr>
          <td class="cell-stat text-center">Accion</td>
        <th class="cell-stat text-center">Accion</td>
      </tr>
    </tbody>
    <tbody>
        <tr>

          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/accesorio/<%= accesorio.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancel
                </button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
