<form id="topicForm" method="post" action="/web/accesorio/<%= accesorio.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= accesorio.id %>">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <th class="cell-stat text-center">id</th>
        <th class="cell-stat text-center">accesorio</th>
        <th class="cell-stat text-center">relacion</th>
        <th class="cell-stat text-center">ObraId</th>
        <th class="cell-stat text-center">creacion</th>
        <th class="cell-stat text-center">modifica</th>
        <th class="cell-stat text-center">borrado</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td class="text-center">
            <%= accesorio.id %>
          </td>
          <td class="text-center">
            <label for="accesorio" class="control-label">ObjetoCodigo</label>
            <input type="text" class="form-control" id="objetoCodigo" name="objetoCodigo" value="<%=accesorio.objetoCodigo%>" required="true" title="Ingrese objetoCodigo" placeholder="<objetoCodigo>">
            <%= accesorio.objetoCodigo %>
          </td>
          <td class="text-center">
            <label for="relacion" class="control-label">Relacion</label>
            <input type="text" class="form-control" id="relacion" name="relacion" value="<%=accesorio.relacion%>" required="true" title="Ingrese relacion" placeholder="<relacion>">
            <%= accesorio.relacion %>
          </td>
          <td class="text-center">
            <%= accesorio.ObraId %>
          </td>
          <td class="text-center"><%= accesorio.creacion %></td>
          <td class="text-center"><%= accesorio.modifica %></td>
          <td class="text-center"><%= accesorio.borrado %></td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/accesorio/<%= accesorio.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancel
                </button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
