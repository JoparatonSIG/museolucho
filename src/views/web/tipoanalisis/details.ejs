<form id="topicForm" method="post" action="/web/tipoanalisis/<%= tipoAnalisisEJS.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= tipoAnalisisEJS.id %>">
  <table class="table table-bordred table-striped">
    <tbody>
      <tr>
        <td class="cell-stat text-right">id :</td>
        <td class="text-left">
          <%= tipoAnalisisEJS.id %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-right">Tipo :</td>
        <td class="text-left">
          <%= tipoAnalisisEJS.tipo %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-right">Sub Tipo:</td>
        <td class="text-left">
          <%= tipoAnalisisEJS.subTipo %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-right">Valor Predeterminado :</td>
        <td class="text-left">
          <%= tipoAnalisisEJS.valorPredeterminado %>
        </td>
      </tr>
      <tr>
        <td>
          <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
            Modificar
          </button>
        </td>
        <td>
          <a href="/web/tipoanalisis/<%= tipoAnalisisEJS.id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Cancel
              </button>
            </p>
          </a>
        </td>
      </tr>
    </tbody>
  </table>
  <table>
    <thead>
      <tr>
        <th class="cell-stat text-center">id</th>
        <th class="cell-stat text-center">TipoAnalisis</th>
        <th class="cell-stat text-center">analisis</th>
        <th class="cell-stat text-center">ObraId</th>
        <th class="cell-stat text-center">creacion</th>
        <th class="cell-stat text-center">Modifica</th>
        <th class="cell-stat text-center">Borrado</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
      <% var i; for (i=0; i < 5; i++) { %>
        <tr>
          <td class="text-center"><%= tipoAnalisisEJS.Analises[i].id %></td>
          <% if (tipoAnalisisEJS.Analises[i].TipoAnalisis == null)  {%>
            <td class="text-center">sin tipo</td>
          <% } else { %>
            <td class="text-center"><%= tipoAnalisisEJS.Analises[i].TipoAnalisis.tipo %></td>
          <% }%>
          <td class="text-center"><%= tipoAnalisisEJS.Analises[i].analisis %></td>
          <td class="text-center"><%= tipoAnalisisEJS.Analises[i].ObraId %></td>
          <td class="text-center"><%= tipoAnalisisEJS.Analises[i].fechaCrea %></td>
          <td class="text-center"><%= tipoAnalisisEJS.Analises[i].fechaModifica %></td>
          <td class="text-center"><%= tipoAnalisisEJS.Analises[i].fechaBorra %></td>
          <td>
            <a href="/web/analisis/<%= tipoAnalisisEJS.Analises[i].id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Edit</button>
              </p>
            </a>
          </td>
          <td>
            <form id="topicForm" method="post" action="/web/analisis/<%= tipoAnalisisEJS.Analises[i].id %>" >
              <input type="hidden" name="_method" value="delete">
              <input type="hidden" name="id" value="<%= tipoAnalisisEJS.Analises[i].id %>">
              <p data-placement="top" data-toggle="tooltip" title="Delete">
                <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
                  Delete
                </button>
              </p>
            </form>
          </td>
        </tr>
      <% } %>
    </tbody>
  </table>
</form>
