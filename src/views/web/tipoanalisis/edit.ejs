<form id="topicForm" method="post" action="/web/tipoanalisis/<%= tipoanalisis.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= tipoanalisis.id %>">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <th class="cell-stat text-center">id</th>
        <th class="cell-stat text-center">tipo</th>
        <th class="cell-stat text-center">subTipo</th>
        <th class="cell-stat text-center">valorPredeterminado</th>
        <th class="cell-stat text-center">fechaCrea</th>
        <th class="cell-stat text-center">fechaModifica</th>
        <th class="cell-stat text-center">fechaBorra</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td class="text-center">
            <%= tipoanalisis.id %>
          </td>
          <td class="text-center">
            <label for="tipo" class="control-label">Tipo</label>
            <input type="text" class="form-control" id="tipo" name="tipo" value="<%=tipoanalisis.tipo%>" required="true" title="Ingrese Tipo analisis" placeholder="<tipo>">
            <%= tipoanalisis.tipo %>
          </td>
          <td class="text-center">
            <label for="subTipo" class="control-label">SubTipo</label>
            <input type="text" class="form-control" id="subTipo" name="subTipo" value="<%=tipoanalisis.subTipo%>" required="true" title="Ingrese subtipo" placeholder="<subTipo>">
            <%= tipoanalisis.subTipo %>
          </td>
          <td class="text-center">
            <label for="valorPredeterminado" class="control-label">Valor predeterminado</label>
            <input type="text" class="form-control" id="valorPredeterminado" name="valorPredeterminado" value="<%=tipoanalisis.valorPredeterminado%>" required="true" title="Ingrese valor predeterminado" placeholder="<valorPredeterminado>">
            <%= tipoanalisis.valorPredeterminado %>
          </td>
          <td class="text-center"><%= tipoanalisis.fechaCrea %></td>
          <td class="text-center"><%= tipoanalisis.fechaModifica %></td>
          <td class="text-center"><%= tipoanalisis.fechaBorra %></td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/tipoanalisis/<%= tipoanalisis.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancel
                </button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
