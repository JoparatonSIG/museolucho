<table class="table table-bordred table-striped">
  <thead>
    <tr>
      <th class="cell-stat text-center">id</th>
      <th class="cell-stat text-center">fechaRelev</th>
      <th class="cell-stat text-center">fechaCatalog</th>
      <th class="cell-stat text-center">fechaRevision</th>
      <th class="cell-stat text-center">quienRelevo</th>
      <th class="cell-stat text-center">quienCatalogo</th>
      <th class="cell-stat text-center">quienReviso</th>
      <th class="cell-stat text-center">observaciones</th>
      <th class="cell-stat text-center">fechaCrea</th>
      <th class="cell-stat text-center">fechaModifica</th>
      <th class="cell-stat text-center">fechaBorra</th>
      <th class="cell-stat text-center">Accion</th>
      <th class="cell-stat text-center">Accion</th>
    </tr>
  </thead>
  <tbody>
    <% var i; for (i=0; i < relevamientos.length; i++) { %>
      <tr>
        <td class="text-center"><%= relevamientos[i].id %></td>
        <td class="text-center"><%= relevamientos[i].fechaRelev %></td>
        <td class="text-center"><%= relevamientos[i].fechaCatalog %></td>
        <td class="text-center"><%= relevamientos[i].fechaRevision %></td>
        <td class="text-center"><%= relevamientos[i].quienRelevo %></td>
        <td class="text-center"><%= relevamientos[i].quienCatalogo %></td>
        <td class="text-center"><%= relevamientos[i].quienReviso %></td>
        <td class="text-center"><%= relevamientos[i].observaciones %></td>
        <td class="text-center"><%= relevamientos[i].fechaCrea %></td>
        <td class="text-center"><%= relevamientos[i].fechaModifica %></td>
        <td class="text-center"><%= relevamientos[i].fechaBorra %></td>
        <td>
          <a href="/web/relevamiento/<%= relevamientos[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Edit</button>
            </p>
          </a>
        </td>
        <td>
          <form id="topicForm" method="post" action="/web/relevamiento/<%= relevamientos[i].id %>" >
            <input type="hidden" name="_method" value="delete">
            <input type="hidden" name="id" value="<%= relevamientos[i].id %>">
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Eliminar
            </button>
          </form>
        </td>

      </tr>
    <% } %>
  </tbody>
</table>
<form id="topicForm" method="get" action="/web/relevamiento/%>" >
    <% if (count > limitPage) { %>
    <% if (totalPage <= 8) { %>
      <% var j; for (j=1; j <= totalPage; j++) { %>
        <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
          <%=j%>
        </button>
      <% } %>
    <% } else { %>
      <% var j; for (j=1; j <= 3; j++) { %>
        <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
          <%=j%>
        </button>
      <% } %>
      <p>...</p>
      <% for (j=totalPage-3; j <= totalPage+1; j++) { %>
        <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
          <%=j%>
        </button>
      <% } %>
    <% } %>
    <% } %>
</form>
