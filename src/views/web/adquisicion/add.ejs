<form id="topicForm" method="post" action="/web/adquisicion/" >
    <table class="table table-bordred table-striped">
      <thead>
        <tr>
          <th class="cell-stat text-center">tipoAdquisicion</th>
          <th class="cell-stat text-center">tipoCompra</th>
          <th class="cell-stat text-center">fecha</th>
        </tr>
      </thead>
      <tbody>
          <tr>
            <td class="text-center">
              <label for="tipoAdquisicion" class="control-label">tipo adquisicion</label>
              <input type="text" class="form-control" id="tipoAdquisicion" name="tipoAdquisicion" value="<%=adquisicion.tipoAdquisicion%>" required="true" title="Ingrese adquisicion" placeholder="<tipoAdquisicion>">
              <%= adquisicion.tipoAdquisicion %>
            </td>
            <td class="text-center">
              <label for="tipoCompra" class="control-label">tipo compra</label>
              <input type="text" class="form-control" id="tipoCompra" name="tipoCompra" value="<%=adquisicion.tipoCompra%>" required="true" title="Ingrese tipo compra" placeholder="<tipoCompra>">
              <%= adquisicion.tipoCompra %>
            </td>
            <td class="text-center">
              <label for="fecha" class="control-label">fecha</label>
              <input type="text" class="form-control" id="fecha" name="fecha" value="<%=adquisicion.fecha%>" required="true" title="Ingrese fecha" placeholder="<fecha>">
              <%= adquisicion.fecha %>
            </td>
          </tr>
      </tbody>
    </table>
    <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
      Agregar
    </button>
</form>
