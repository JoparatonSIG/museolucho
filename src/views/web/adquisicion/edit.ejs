<form id="topicForm" method="post" action="/web/adquisicion/<%= adquisicion.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= adquisicion.id %>">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <th class="cell-stat text-center">id</th>
        <th class="cell-stat text-center">tipoAdquisicion</th>
        <th class="cell-stat text-center">tipoCompra</th>
        <th class="cell-stat text-center">fecha</th>
        <th class="cell-stat text-center">fechaCrea</th>
        <th class="cell-stat text-center">fechaModifica</th>
        <th class="cell-stat text-center">fechaBorra</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td class="text-center">
            <%= adquisicion.id %>
          </td>
          <td class="text-center">
            <label for="tipoAdquisicion" class="control-label">Adquisicion</label>
            <input type="text" class="form-control" id="tipoAdquisicion" name="tipoAdquisicion" value="<%=adquisicion.tipoAdquisicion%>" required="true" title="Ingrese adquisici√≥n" placeholder="<tipoAdquisicion>">
            <%= adquisicion.tipoAdquisicion %>
          </td>
          <td class="text-center">
            <label for="tipoCompra" class="control-label">Tipo de Compra</label>
            <input type="text" class="form-control" id="tipoCompra" name="tipoCompra" value="<%=adquisicion.tipoCompra%>" required="true" title="Ingrese tipo de compra" placeholder="<tipoCompra>">
            <%= adquisicion.tipoCompra %>
          </td>
          <td class="text-center">
            <label for="fecha" class="control-label">Fecha</label>
            <input type="text" class="form-control" id="fecha" name="fecha" value="<%=adquisicion.fecha%>" required="true" title="Ingrese fecha" placeholder="<fecha>">
            <%= adquisicion.fecha %>
          </td>
          <td class="text-center"><%= adquisicion.fechaCrea %></td>
          <td class="text-center"><%= adquisicion.fechaModifica %></td>
          <td class="text-center"><%= adquisicion.fechaBorra %></td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/adquisicion/<%= adquisicion.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancel
                </button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
