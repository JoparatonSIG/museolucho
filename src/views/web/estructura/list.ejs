<table class="table table-bordred table-striped">
  <thead>
    <tr>
      <th class="cell-stat text-center">id</th>
      <th class="cell-stat text-center">estructura</th>
      <th class="cell-stat text-center">fechaCrea</th>
      <th class="cell-stat text-center">fechaModifica</th>
      <th class="cell-stat text-center">fechaBorra</th>
      <th class="cell-stat text-center">Accion</th>
      <th class="cell-stat text-center">Accion</th>
    </tr>
  </thead>
  <tbody>
    <% var i; for (i=0; i < estructuras.length; i++) { %>
      <tr>
        <td class="text-center"><%= estructuras[i].id %></td>
        <td class="text-center"><%= estructuras[i].estructura %></td>
        <td class="text-center"><%= estructuras[i].fechaCrea %></td>
        <td class="text-center"><%= estructuras[i].fechaModifica %></td>
        <td class="text-center"><%= estructuras[i].fechaBorra %></td>
        <td>
          <a href="/web/estructura/<%= estructuras[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Edit</button>
            </p>
          </a>
        </td>
        <td>
          <a href="/web/estructura/<%= estructuras[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Delete</button>
            </p>
          </a>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>
