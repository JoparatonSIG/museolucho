<form id="topicForm" method="post" action="/web/estructura/<%= estructura.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= estructura.id %>">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <th class="cell-stat text-center">id</th>
        <th class="cell-stat text-center">estructura</th>
        <th class="cell-stat text-center">fechaCrea</th>
        <th class="cell-stat text-center">fechaModifica</th>
        <th class="cell-stat text-center">fechaBorra</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td class="text-center">
            <%= estructura.id %>
          </td>
          <td class="text-center">
            <label for="estructura" class="control-label">estructura</label>
            <input type="text" class="form-control" id="estructura" name="estructura" value="<%=estructura.estructura%>" required="true" title="Ingrese estructura" placeholder="<estructura>">
            <%= estructura.estructura %>
          </td>
          <td class="text-center"><%= estructura.fechaCrea %></td>
          <td class="text-center"><%= estructura.fechaModifica %></td>
          <td class="text-center"><%= estructura.fechaBorra %></td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/estructura/<%= estructura.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancel
                </button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
