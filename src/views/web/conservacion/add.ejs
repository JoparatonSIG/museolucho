<form id="topicForm" method="post" action="/web/conservacion/<%= conservacion.id %>" >
    <table class="table table-bordred table-striped">
      <tbody>
        <tr>
          <td class="cell-stat text-center">Conservacion</td>
            <td class="text-center">
              <input type="text" class="form-control" id="conservacion" name="conservacion" value="<%=conservacion.conservacion%>" required="true" title="Ingrese conservacion" placeholder="<conservacion>">
              <%= conservacion.conservacion %>
            </td>
        </tr>
          <tr>
          <td class="cell-stat text-center">Condiciones Seguridad</td>
            <td class="text-center">
              <input type="text" class="form-control" id="condicionesSeguridad" name="condicionesSeguridad" value="<%=conservacion.condicionesSeguridad%>" required="true" title="Ingrese condicionesSeguridad" placeholder="<condicionesSeguridad>">
              <%= conservacion.condicionesSeguridad %>
            </td>
          </tr>
      </tbody>
    </table>
    <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
      Agregar
    </button>
</form>
