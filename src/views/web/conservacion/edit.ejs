<form id="topicForm" method="post" action="/web/conservacion/<%= conservacion.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= conservacion.id %>">

  <table class="table table-bordred table-striped">
    <tbody>
      <tr>
        <td class="cell-stat text-center">id</td>
        <td class="text-center">
            <%= conservacion.id %>
        </td>
      </tr>
      <tr>
          <td class="cell-stat text-center">Conservacion</td>
          <td class="text-center">
            <input type="text" class="form-control" id="conservacion" name="conservacion" value="<%=conservacion.conservacion%>" required="true" title="Ingrese conservacion" placeholder="<conservacion>">
            <%= conservacion.conservacion %>
          </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Condiones de Seguridad</td>
        <td class="text-center">
          <input type="text" class="form-control" id="condicionesSeguridad" name="condicionesSeguridad" value="<%=conservacion.condicionesSeguridad%>" required="true" title="Ingrese Condiciones de Seguridad" placeholder="<condicionesSeguridad>">
          <%= conservacion.condicionesSeguridad %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">ObraId</td>
        <td class="text-center">
          <%= conservacion.ObraId %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">FechaCrea</td>
        <td class="text-center">
          <%= conservacion.fechaCrea %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">FechaModifica</td>
        <td class="text-center">
          <%= conservacion.fechaModifica %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">FechaBorra</td>
        <td class="text-center">
          <%= conservacion.fechaBorra %>
        </td>
      <tr>
          <td class="cell-stat text-center">Accion</td>
        <th class="cell-stat text-center">Accion</td>
      </tr>
    </tbody>
    <tbody>
        <tr>

          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/conservacion/<%= conservacion.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancel
                </button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
