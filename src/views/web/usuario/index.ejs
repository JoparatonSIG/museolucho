<form id="topicForm" method="post" action="/web/usuario/cargar/<%= usuarioJ.id %>" >
    <table class="table table-bordred table-striped">
      <thead>
        <tr>
          <th class="cell-stat text-center">Email</th>
          <th class="cell-stat text-center">Nombre</th>          
          <th class="cell-stat text-center">Apellido</th>
          <th class="cell-stat text-center">Password</th>          
          <th class="cell-stat text-center">Nivel</th>
        </tr>
      </thead>
      <tbody>
          <tr>
          	<td class="text-center">
              <label for="email" class="control-label">Email</label>
              <input type="text" class="form-control" id="email" name="email" value="<%=usuarioJ.email%>" required="true" title="Ingrese email" placeholder="<email>">
              <%= usuarioJ.email %>
            </td>

            <td class="text-center">
              <label for="nombre" class="control-label">Nombre</label>
              <input type="text" class="form-control" id="nombre" name="nombre" value="<%=usuarioJ.nombre%>" required="true" title="Ingrese nombre" placeholder="<nombre>">
              <%= usuarioJ.nombre %>
            </td>
            <td class="text-center">
              <label for="apellido" class="control-label">Apellido</label>
              <input type="text" class="form-control" id="apellido" name="apellido" value="<%=usuarioJ.apellido%>" required="true" title="Ingrese apellido" placeholder="<apellido>">
              <%= usuarioJ.apellido %>
            </td>

            <td class="text-center">
              <label for="password" class="control-label">Password</label>
              <input type="text" class="form-control" id="password" name="password" value="<%=usuarioJ.password%>" required="true" title="Ingrese password" placeholder="<password>">
              <%= usuarioJ.password %>
            </td>

            <td class="text-center">
              <select name="selectJ">
      		      <option value="-1" selected>Seleccione Nivel</option>
      		      <% var j; for (j=0; j < selectJ.length; j++) { %>
        		       <option value="<%= selectJ[j].id %>"><%= selectJ[j].categoria %></option>
      		      <% } %>
              </select>
            </td>            
          </tr>
      </tbody>
    </table>
    <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
      Agregar
    </button>
</form>
