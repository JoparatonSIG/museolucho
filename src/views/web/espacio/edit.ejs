<form id="topicForm" method="post" action="/web/espacio/<%= espacio.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= espacio.id %>">

  <table class="table table-bordred table-striped">
    <thead>
      <tr>
        <th class="cell-stat text-center">id</th>
        <th class="cell-stat text-center">espacio</th>
        <th class="cell-stat text-center">codigoEspacio</th>
        <th class="cell-stat text-center">inmuebles</th>
        <th class="cell-stat text-center">codigoInmueble</th>
        <th class="cell-stat text-center">ubicacionInmueble</th>
        <th class="cell-stat text-center">fechaCrea</th>
        <th class="cell-stat text-center">fechaModifica</th>
        <th class="cell-stat text-center">fechaBorra</th>
        <th class="cell-stat text-center">Accion</th>
        <th class="cell-stat text-center">Accion</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td class="text-center">
            <%= espacio.id %>
          </td>
          <td class="text-center">
            <label for="espacio" class="control-label">Espacio</label>
            <input type="text" class="form-control" id="espacio" name="espacio" value="<%=espacio.espacio%>" required="true" title="Ingrese espacio" placeholder="<espacio>">
            <%= espacio.espacio %>
          </td>
          <td class="text-center">
            <label for="codigoEspacio" class="control-label">codigoEspacio</label>
            <input type="text" class="form-control" id="codigoEspacio" name="codigoEspacio" value="<%=espacio.codigoEspacio%>" required="true" title="Ingrese codigoEspacio" placeholder="<codigoEspacio>">
            <%= espacio.codigoEspacio %>
          </td>
          <td class="text-center">
            <label for="inmuebles" class="control-label">inmuebles</label>
            <input type="text" class="form-control" id="inmuebles" name="inmuebles" value="<%=espacio.inmuebles%>" required="true" title="Ingrese inmuebles" placeholder="<inmuebles>">
            <%= espacio.inmuebles %>
          </td><td class="text-center">
            <label for="codigoInmueble" class="control-label">codigoInmueble</label>
            <input type="text" class="form-control" id="codigoInmueble" name="codigoInmueble" value="<%=espacio.codigoInmueble%>" required="true" title="Ingrese codigoInmueble" placeholder="<codigoInmueble>">
            <%= espacio.codigoInmueble %>
          </td>
          </td><td class="text-center">
            <label for="ubicacionInmueble" class="control-label">ubicacionInmueble</label>
            <input type="text" class="form-control" id="ubicacinoInmueble" name="ubicacionInmueble" value="<%=espacio.ubicacionInmueble%>" required="true" title="Ingrese ubicacionInmueble" placeholder="<ubicacionInmueble>">
            <%= espacio.ubicacionInmueble %>
          </td>
          <td class="text-center"><%= espacio.fechaCrea %></td>
          <td class="text-center"><%= espacio.fechaModifica %></td>
          <td class="text-center"><%= espacio.fechaBorra %></td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/espacio/<%= espacio.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancel
                </button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
