<form id="topicForm" method="post" action="/web/intervencion/" >
    <table class="table table-bordred table-striped">
      <tbody>

        <tr>
          <td class="cell-stat text-center">metodologia</td>
            <td class="text-center">
              <input type="text" class="form-control" id="metodologia" name="metodologia" value="<%=intervencion.metodologia%>" required="true" title="Ingrese metodologia" placeholder="<metodologia>">
              <%= intervencion.metodologia %>
            </td>
        </tr>
          <tr>
          <td class="cell-stat text-center">fechaRestauracion</td>
            <td class="text-center">
              <input type="text" class="form-control" id="fechaRestauracion" name="fechaRestauracion" value="<%=intervencion.fechaRestauracion%>" required="true" title="Ingrese fecha restauracion" placeholder="<fechaRestauracion>">
              <%= intervencion.fechaRestauracion %>
            </td>
          </tr>
          <tr>
          <td class="cell-stat text-center">apellidoRestaurador</td>
            <td class="text-center">
              <input type="text" class="form-control" id="apellidoRestaurador" name="apellidoRestaurador" value="<%=intervencion.apellidoRestaurador%>" required="true" title="Ingrese apellido del restaurador" placeholder="<apellidoRestaurador>">
              <%= intervencion.apellidoRestaurador %>
            </td>
          </tr>
          <tr>
          <td class="cell-stat text-center">descripcion</td>
            <td class="text-center">
              <input type="text" class="form-control" id="descripcion" name="descripcion" value="<%=intervencion.descripcion%>" required="true" title="Ingrese descripcion" placeholder="<descripcion>">
              <%= intervencion.descripcion %>
            </td>
          </tr>
      </tbody>
    </table>
    <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
      Agregar
    </button>
</form>
