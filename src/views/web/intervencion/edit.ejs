<form id="topicForm" method="post" action="/web/intervencion/<%= intervencion.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= intervencion.id %>">

  <table class="table table-bordred table-striped">
    <tbody>
      <tr>
        <td class="cell-stat text-center">id</td>
          <td class="text-center">
            <%= intervencion.id %>
          </td>
      </tr>
        <tr>
        <td class="cell-stat text-center">metodologia</td>
          <td class="text-center">
            <input type="text" class="form-control" id="metodologia" name="metodologia" value="<%=intervencion.metodologia%>" required="true" title="Ingrese metodologia" placeholder="<metodologia>">
            <%= intervencion.metodologia %>
          </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">fechaRestauracion</td>
          <td class="text-center">
            <input type="text" class="form-control" id="fechaRestauracion" name="fechaRestauracion" value="<%=intervencion.fechaRestauracion%>" required="true" title="Ingrese fechaRestauracion" placeholder="<fechaRestauracion>">
            <%= intervencion.fechaRestauracion %>
          </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">apellidoRestaurador</td>
          <td class="text-center">
            <input type="text" class="form-control" id="apellidoRestaurador" name="apellidoRestaurador" value="<%=intervencion.apellidoRestaurador%>" required="true" title="Ingrese apellidoRestaurador" placeholder="<apellidoRestaurador>">
            <%= intervencion.apellidoRestaurador %>
          </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">descripcion</td>

          <td class="text-center">
            <input type="text" class="form-control" id="descripcion" name="descripcion" value="<%=intervencion.descripcion%>" required="true" title="Ingrese descripcion" placeholder="<descripcion>">
            <%= intervencion.descripcion %>
          </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">fechaCrea</td>
          <td class="text-center"><%= intervencion.fechaCrea %></td>
      </tr>
      <tr>
        <td class="cell-stat text-center">fechaModifica</td>
          <td class="text-center"><%= intervencion.fechaModifica %></td>
      </tr>
      <tr>
        <td class="cell-stat text-center">fechaBorra</td>
          <td class="text-center"><%= intervencion.fechaBorra %></td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Accion</td>
          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Accion</td>

          <td>
            <a href="/web/intervencion/<%= intervencion.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancel
                </button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
