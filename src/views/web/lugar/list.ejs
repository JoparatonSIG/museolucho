<table class="table table-bordred table-striped">
  <thead>
    <tr>
      <th class="cell-stat text-center">id</th>
      <th class="cell-stat text-center">localidad</th>
      <th class="cell-stat text-center">codigoLocalidad</th>
      <th class="cell-stat text-center">municipio</th>
      <th class="cell-stat text-center">provincia</th>
      <th class="cell-stat text-center">codigoProvincia</th>
      <th class="cell-stat text-center">departamento</th>
      <th class="cell-stat text-center">creacion</th>
      <th class="cell-stat text-center">Modifica</th>
      <th class="cell-stat text-center">Borrado</th>
      <th class="cell-stat text-center">Accion</th>
      <th class="cell-stat text-center">Accion</th>
    </tr>
  </thead>
  <tbody>
    <% var i; for (i=0; i < lugar.length; i++) { %>
      <tr>
        <td class="text-center"><%= lugar[i].id %></td>
        <td class="text-center"><%= lugar[i].localidad %></td>
        <td class="text-center"><%= lugar[i].codigoLocalidad %></td>
        <td class="text-center"><%= lugar[i].municipio %></td>
        <td class="text-center"><%= lugar[i].provincia %></td>
        <td class="text-center"><%= lugar[i].codigoProvincia %></td>
        <td class="text-center"><%= lugar[i].departamento %></td>
        <td class="text-center"><%= lugar[i].fechacrea %></td>
        <td class="text-center"><%= lugar[i].fechamodifica %></td>
        <td class="text-center"><%= lugar[i].fechaborra %></td>
        <td>
          <a href="/web/lugar/<%= lugar[i].id %>" class="btn btn-info" role="button">
            <p data-placement="top" data-toggle="tooltip" title="Edit">
              <button type="button" class="btn btn-info">Edit</button>
            </p>
          </a>
        </td>
        <td>
          <form id="topicForm" method="post" action="/web/lugar/<%= lugar[i].id %>" >
            <input type="hidden" name="_method" value="delete">
            <input type="hidden" name="id" value="<%= lugar[i].id %>">
            <p data-placement="top" data-toggle="tooltip" title="Delete">
              <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
                Delete
              </button>
            </p>
          </form>
        </td>
      </tr>
    <% } %>
  </tbody>
</table>
<form id="topicForm" method="get" action="/web/lugar/%>" >
    <% if (count > limitPage) { %>
      <% if (totalPage <= 8) { %>
        <% var j; for (j=0; j <= totalPage; j++) { %>
          <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
            <%=j%>
          </button>
        <% } %>
      <% } else { %>
        <% var j; for (j=1; j <= 3; j++) { %>
          <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
            <%=j%>
          </button>
        <% } %>
        <p>...</p>
        <% for (j=totalPage-3; j <= totalPage+1; j++) { %>
          <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
            <%=j%>
          </button>
        <% } %>
      <% } %>
    <% } %>
</form>
<a href="/web/lugar/add %>" class="btn btn-info" role="button">
  <p data-placement="top" data-toggle="tooltip" title="Edit">
    <button type="button" class="btn btn-info">Agregar</button>
  </p>
</a>
