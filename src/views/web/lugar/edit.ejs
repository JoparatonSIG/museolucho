<form id="topicForm" method="post" action="/web/lugar/<%= lugarEJS.id %>" >
  <input type="hidden" name="_method" value="put">
  <input type="hidden" name="id" value="<%= lugarEJS.id %>">

  <table class="table table-bordred table-striped">
    <tbody>
      <tr>
        <td class="cell-stat text-center">id</td>
        <td class="text-center">
            <%= lugarEJS.id %>
        </td>
      </tr>
      <tr>
          <td class="cell-stat text-center">Localidad</td>
          <td class="text-center">
            <input type="text" class="form-control" id="localidad" name="localidad" value="<%=lugarEJS.localidad%>" required="true" title="Ingreselocalidad" placeholder="<localidad>">
            <%= lugarEJS.localidad %>
          </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Codigo de la Localidad</td>
        <td class="text-center">
          <input type="text" class="form-control" id="codigoLocalidad" name="codigoLocalidad" value="<%=lugarEJS.codigoLocalidad%>" required="true" title="Ingrese codigoLocalidad" placeholder="<codigoLocalidad>">
          <%= lugarEJS.codigoLocalidad %>
        </td>
      </tr>

      <tr>
          <td class="cell-stat text-center">Municipio</td>
          <td class="text-center">
            <input type="text" class="form-control" id="municipio" name="municipio" value="<%=lugarEJS.municipio%>" required="true" title="Ingreselocalidad" placeholder="<municipio>">
            <%= lugarEJS.municipio %>
          </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Provincia</td>
        <td class="text-center">
          <input type="text" class="form-control" id="provincia" name="provincia" value="<%=lugarEJS.provincia%>" required="true" title="Ingrese provincia" placeholder="<provincia>">
          <%= lugarEJS.provincia %>
        </td>
      </tr>
      <tr>
          <td class="cell-stat text-center">Codigo Provincia</td>
          <td class="text-center">
            <input type="text" class="form-control" id="codigoProvincia" name="codigoProvincia" value="<%=lugarEJS.codigoProvincia%>" required="true" title="Ingrese Codigo Provincia" placeholder="<codigoProvincia>">
            <%= lugarEJS.codigoProvincia %>
          </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">Departamento</td>
        <td class="text-center">
          <input type="text" class="form-control" id="departamento" name="departamento" value="<%=lugarEJS.departamento%>" required="true" title="Ingrese departamento" placeholder="<departamento>">
          <%= lugarEJS.departamento %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">FechaCrea</td>
        <td class="text-center">
          <%= lugarEJS.fechaCrea %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">FechaModifica</td>
        <td class="text-center">
          <%= lugarEJS.fechaModifica %>
        </td>
      </tr>
      <tr>
        <td class="cell-stat text-center">FechaBorra</td>
        <td class="text-center">
          <%= lugarEJS.fechaBorra %>
        </td>
      <tr>
          <td class="cell-stat text-center">Accion</td>
        <th class="cell-stat text-center">Accion</td>
      </tr>
    </tbody>
    <tbody>
        <tr>

          <td>
            <button type="submit" name="commit" value="Form" class="btn btn-success btn-block">
              Modificar
            </button>
          </td>
          <td>
            <a href="/web/lugar/<%= lugarEJS.id %>" class="btn btn-info" role="button">
              <p data-placement="top" data-toggle="tooltip" title="Edit">
                <button type="button" class="btn btn-info">Cancel
                </button>
              </p>
            </a>
          </td>
        </tr>
    </tbody>
  </table>
</form>
